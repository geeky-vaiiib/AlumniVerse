{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-duplicate-imports": "error",
    "no-redeclare": "error",
    "no-undef": "error",
    "react/no-unescaped-entities": "warn",
    "react/jsx-no-duplicate-props": "error",
    "react/jsx-key": "warn",
    "react/jsx-no-undef": "error",
    "react/no-unused-prop-types": "warn",
    "react/prop-types": "off"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
